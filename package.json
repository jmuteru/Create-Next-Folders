{
  "type": "module",
  "name": "create-next-folders",
  "version": "2.0.0",
  "description": "A CLI tool to scaffold Next.js or React projects with customizable folder structures.",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "scaffold": "create-next-folders"
  },
  "dependencies": {
    "chalk": "^5.0.0",
    "prompts": "^2.4.1",
    "fs-extra": "^10.0.0"
  },
  "bin": {
    "create-next-folders": "./src/index.js"
  },
  "files": [
    "src/**/*"
  ],
  "keywords": [
    "react",
    "next.js",
    "cli",
    "scaffold",
    "project-structure",
    "folder-structure",
    "boilerplate"
  ],
  "author": "Jeff Muteru",
  "contributors": [
    "Valentine Wabwire <Github> (https://github.com/wabwirevalentine)"
  ],
  "license": "MIT"
}
